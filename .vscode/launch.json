	"version": "0.2.0",
	"configurations": [
			{
					"name": "Launch",
					"type": "node",
					"request": "launch",
					"program": "${workspaceRoot}/src/LocalSkillServer.js",
//					"program": "${workspaceRoot}/src/server.js",
					"stopOnEntry": false,
					"args": [],
					"cwd": "${workspaceRoot}",
					"preLaunchTask": null,
					"runtimeExecutable": null,
